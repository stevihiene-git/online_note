{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "note_app/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/note_app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
 ],
